<?xml version="1.0"?>
<config>
    <modules>
        <RapidCommerce_Ias>
            <version>1.0.0</version>
        </RapidCommerce_Ias>
    </modules>
    <frontend>
        <layout>
            <updates>
                <rapidcommerce_ias>
                    <file>rapidcommerce-ias.xml</file>
                </rapidcommerce_ias>
            </updates>
        </layout>
		<translate>
			<modules>
				<files>
					<Rapidcommerce_Ias>
						<default>RapidCommerce_Ias.csv</default>
					</Rapidcommerce_Ias>
				</files>
			</modules>
		</translate>
     </frontend>
	 <global>
		<blocks>
			<ias>
				<class>RapidCommerce_Ias_Block</class>
			</ias>
		</blocks>
	</global>
	<default>
		<catalog>
			<frontend>
				
				<useajaxscroll>0</useajaxscroll>
				
				<useuitotop>0</useuitotop>
			</frontend>
		</catalog>
	</default>
</config>

